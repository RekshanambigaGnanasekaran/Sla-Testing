package slaTask;

class Calculator{
	public int add(int a,int b) {
		int c = a + b;
		return c;
	}
	
	public int sub(int d, int e) {
		int f = d - e;
		return f;
	}
	
	public int multiply(int g, int h) {
		int i = g * h;
		return i;
	}
	
	public int div(int j,int k) {
		int l = j/k;
		return l;
	}
}

public class CalculatorUsingOverload {	
	public static void main(String[] args) {
		Calculator value = new Calculator();
		int additionValue = value.add(10, 5);
		System.out.println("Addition value is : " + additionValue);
		int subValue = value.sub(20, 15);
		System.out.println("Subtraction value is : " + subValue);
		int multiValue = value.multiply(5, 8);
		System.out.println("Multiplication value is : " + multiValue);
		int divValue = value.div(10, 2);
		System.out.println("Division value is : " + divValue);
	}

}
